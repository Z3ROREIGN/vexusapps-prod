{
  "name": "vexusapps",
  "version": 2,
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "public": true,
  "env": {
    "VITE_APP_TITLE": "VexusApps - Automação e Produtos Digitais",
    "VITE_APP_LOGO": "https://private-us-east-1.manuscdn.com/sessionFile/GsS744heNXbVvIqZscrjla/sandbox/KaaKTxIrt5LQUK727e4wQK-img-1_1771980173000_na1fn_dmV4dXNhcHBzLWxvZ28.png"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/public/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
